runTest {test string-97.1 {string map with all byte values} -body {
  #
  # NOTE: Make sure that every byte value can be handled by [string map].
  #
  set result [list]

  for {set i 0} {$i < 256} {incr i} {
    if {[isEagle]} then {
      set c [subst \\x[string format "{0:X}" $i]]
    } else {
      set c [format %c $i]
