proc matchBadTypeError { result } {
  if {[string is integer -strict $result]} then {
    return $result
  }

  if {[isEagle]} then {
    if {[string match \
        {unsupported operand type for operator "*" (*)} $result]} then {
      return badType
    } else {
      return $result
    }
  } else {
    if {[string match \
        {can't use floating-point value as operand of "*"} $result]} then {
      return badType
    } else {
      return $result
    }
  }
}

