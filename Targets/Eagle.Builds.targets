  <!--
  ******************************************************************************
  **                             Eagle Test Types                             **
  ******************************************************************************
  -->

  <!--
      HACK: Apparently, we must be quiet with v3.5 or higher of MSBuild because
            it sees any output to the standard error stream as an indication it
            should proceed to fail the entire build process.  There is supposed
            to be a workaround by setting the "IgnoreStandardErrorWarningFormat"
            property of the "Exec" task to true; however, some reports seem to
            indicate that this is a flaky workaround, at best.  Also, it is not
            available with the .NET Framework 2.0 version of MSBuild; therefore,
            we cannot use it anyway.  Also, disable various tests that interact
            poorly with MSBuild, such as those requiring network access, an
            interactive desktop session, integration via COM, or very precise
            timing.  Finally, set the stop-on-failure flag for the test suite so
            that we halt the entire build process immediately upon encountering
            the first test failure.
  -->
  <PropertyGroup Condition="'$(EagleTestType)' != ''">
    <EagleCommonTestArguments Condition="'$(EagleCommonTestArguments)' == ''">-namespaces true -anyInitialize &quot;set ::no(speech) true; set ::no(excel) true; set ::no(preciseTiming) true; set ::no(temporaryTestLog) true; set ::test_stop_on_failure true; set ::test_stop_on_leak true; object invoke Interpreter.GetActive Quiet true; object invoke -flags +NonPublic Interpreter.GetActive SetNoBackgroundError true; catch {source [file join $::env(XDG_STARTUP_HOME) releaseTestingHelper.eagle]; setupForReleaseTesting {$(EagleTestNotFile)} {$(EagleTestFile)} {$(EagleTestType)} {$(EagleTestAll)}}&quot;</EagleCommonTestArguments>
  </PropertyGroup>

  <!--
  ******************************************************************************
  -->

  <!--
      NOTE: For all non-default builds, set the build suffix for this test run
            (primarily for the benefit of the test log file).
  -->
  <PropertyGroup Condition="'$(EagleTestType)' != '' And
                            '$(EagleTestType)' != 'Default' And
                            '$(EagleTestType)' != 'NetFx20'">
    <EagleTestArguments Condition="'$(EagleTestArguments)' == ''">-anyInitialize &quot;set ::eagle_platform(suffix) $(EagleConfigurationSuffix)&quot;</EagleTestArguments>
  </PropertyGroup>

  <!--
  ******************************************************************************
  -->

  <!--
      HACK: For all non-default builds, we want to show all leaked information,
            except assemblies and processes, which are not counted against the
            running test suite.
  -->
  <PropertyGroup Condition="'$(EagleTestType)' != ''">
    <!--
    <EagleTestFlags>$(EagleTestFlags) -listedLeaks &quot;afters variables commands procedures namespaces files temporaryFiles channels aliases interpreters environment loaded previousPid scopes assemblies processes objects objectCallbacks objectTypes objectInterfaces objectNamespaces connections transactions modules delegates tcl tclInterps tclThreads tclCommands scriptThreads&quot;</EagleTestFlags>
    -->
    <EagleTestFlags>$(EagleTestFlags) -listedLeaks &quot;afters variables commands procedures namespaces files temporaryFiles channels aliases interpreters environment loaded previousPid scopes objects objectCallbacks objectTypes objectInterfaces objectNamespaces connections transactions modules delegates tcl tclInterps tclThreads tclCommands scriptThreads&quot;</EagleTestFlags>
  </PropertyGroup>
